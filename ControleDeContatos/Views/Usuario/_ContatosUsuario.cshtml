@model List<ContatoModel>;

<table class="table" id="contatos-table-usuario">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Nome</th>
            <th scope="col">E-mail</th>
            <th scope="col">Celular</th>
        </tr>
    </thead>
    <tbody>
        @if(Model != null && Model.Any())
        {
            foreach (var contato in Model)
            {
                <tr>
                    <th scope="row">@contato.Id</th>
                    <td>@contato.Name</td>
                    <td>@contato.Email</td>
                    <td>@contato.Fone</td>
                </tr>

            }
        }
        else
        {
            <tr class="odd">
                <td colspan="4" valign="top">Nenhum contato encontrado!!</td>
            </tr>
        }

    </tbody>
</table>